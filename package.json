{
  "name": "alertNeighbourhood",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install-all": "npm install --workspaces",
    "dev": "concurrently \"npm:start --workspace=server\" \"npm:start --workspace=client\"",
    "start-server": "npm --workspace=server start",
    "start-client": "npm --workspace=client start"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
